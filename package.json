{
  "name": "vscode-storybook",
  "displayName": "Storybook",
  "description": "A Storybook extension for Visual Studio Code",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.40.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:extension.helloWorld"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "extension.helloWorld",
        "title": "Hello World"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile",
    "test": "node ./out/test/runTest.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/alexravenna/vscode-storybook.git"
  },
  "keywords": [
    "storybook",
    "vscode"
  ],
  "author": "Alexander Ravenna",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/alexravenna/vscode-storybook/issues"
  },
  "homepage": "https://github.com/alexravenna/vscode-storybook#readme",
  "devDependencies": {
    "@types/glob": "^7.1.1",
    "@types/mocha": "^5.2.6",
    "@types/node": "^10.12.21",
    "@types/vscode": "^1.40.0",
    "glob": "^7.1.4",
    "mocha": "^6.1.4",
    "typescript": "^3.3.1",
    "tslint": "^5.12.1",
    "vscode-test": "^1.2.0"
  }
}